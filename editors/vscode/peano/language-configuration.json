{
  "comments": {
    "lineComment": "#",
    "blockComment": ["/*", "*/"]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["<", ">"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"", "notIn": ["string"] },
    { "open": "'", "close": "'", "notIn": ["string"] },
    { "open": "`", "close": "`", "notIn": ["string"] }
  ],
  "autoCloseBefore": ";:.,=}])>\"'\\n\\t",
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*#region\\b",
      "end": "^\\s*#endregion\\b"
    }
  },
  "wordPattern": "(-?\\d*\\.?\\w+)"
}
